        <TextField fx:id="tfPaisCliente" layoutX="550.0" layoutY="260.0" prefHeight="22.0" prefWidth="180.0" promptText="Pais" stylesheets="@estilo.css" />
                  
        <TableView fx:id="tablaBusquedaCliente" layoutX="22.0" layoutY="49.0" prefHeight="291.0" prefWidth="424.0">
            <columns>
                <TableColumn fx:id="tCnombreCliente" prefWidth="185.0" text="Nombre" />
                <TableColumn fx:id="tCcontacto" prefWidth="85.0" text="Contacto" />
                <TableColumn fx:id="tCcargoContacto" prefWidth="80.0" text="Cargo Contacto" />
                <TableColumn fx:id="tCdireccion" prefWidth="80.0" text="Direccion" />
                <TableColumn fx:id="tCciudad" prefWidth="80.0" text="Ciudad" />
                <TableColumn fx:id="tCpais" prefWidth="80.0" text="País" />
                <TableColumn fx:id="tCtelefono" prefWidth="80.0" text="Teléfono" />
            </columns>
        </TableView>
        <Button fx:id="bt_nuevoCliente" layoutX="465.0" layoutY="318.0" mnemonicParsing="true" onAction="#nuevoClienteFX" text="_Nuevo cliente" />
        <Button fx:id="bt_editarTexto" layoutX="571.0" layoutY="318.0" mnemonicParsing="true" onAction="#editarTextoFX" text="_Editar" />
        <Button fx:id="bt_borrarRegistro" layoutX="683.0" layoutY="318.0" mnemonicParsing="true" onAction="#borrarRegistroFX" text="_Borrar" />
