        <Button fx:id="bt_guardarEditar" layoutX="674.0" layoutY="318.0" mnemonicParsing="false" onAction="#guardarEditarFX" text="Guardar" />
        <Button fx:id="bt_cancelarNuevoCliente" layoutX="475.0" layoutY="318.0" mnemonicParsing="false" onAction="#cancelarNuevoClienteFX" text="Cancelar" />
        <Button fx:id="bt_guardarNuevoRegistro" layoutX="674.0" layoutY="318.0" mnemonicParsing="false" onAction="#guardarNuevoRegistroFX" text="Guardar" />
        <Button fx:id="bt_refresh" layoutX="-80.0" layoutY="363.0" mnemonicParsing="false" onAction="#actualizaTablaBusqueda" text="Refresh" />
